{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*.ts", "**/*.tsx", "app/screens/HomeScreen.tsx", "app/screens/LoginScreen.tsx", "app/screens/RegisterScreen.js"],
  "exclude": ["node_modules"]
}
